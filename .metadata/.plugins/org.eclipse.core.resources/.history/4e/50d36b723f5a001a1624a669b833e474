package zweiteEinheit.Schleifen.copy;

public class BenutzerSchleife {

	public static void main(String[] args) {
		int start = 0;
		int ende = 10;
		
		//bis 10 rechnen
		for (int i = start; i < ende; i++) {
			
		}
		
		//abziehen von 10
		for (int i = 10; i > start; i--) {
			System.out.println(i);
		//Thread.sleep(1000);
		}

	}

}
