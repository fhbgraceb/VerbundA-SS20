package zweiteHausuebung.Schleifen;

public class h_NMalN {
	/*
	Aufgabe:
	8.AllePrimzahlen: Lesen Sie eine Zahl ein 
	und geben Sie alle Primzahlen aus, 
	die kleiner als diese Zahl sind.
	*/
	
	public static long nMalN(long n){
		long summe = 0;
		
		for (long i = 1; i <= n; i++)
		{
			summe += i * n;
		}
		
		return summe;
	}
}
